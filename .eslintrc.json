{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "no-shadow": "off",
    "semi": ["error", "never"]
  }
}
